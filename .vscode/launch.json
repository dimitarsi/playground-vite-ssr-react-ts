  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Prod Server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\src\\server\\prod.js",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Dev Server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\src\\server\\dev.js",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
